    <Color x:Key="AppBackgroundColor">Black</Color>
    <Color x:Key="AppForegroundColor">White</Color>

    <Color x:Key="AppBarBackgroundColor">#FF171717</Color>
    <Color x:Key="AppBarForegroundColor">White</Color>

    <Color x:Key="PageTitleForegroundColor">White</Color>
