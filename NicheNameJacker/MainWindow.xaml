                </StackPanel>

                <Popup x:Name="InfoPopup" PopupAnimation="Slide" Placement="Top"
					PlacementTarget="{Binding ElementName=InfoButton}" 
					StaysOpen="False" 
                       HorizontalOffset="{Binding ActualWidth, ConverterParameter=37 - @VALUE, Converter={StaticResource MathConverter}, ElementName=border, Mode=OneWay}" 
                       AllowsTransparency="True" VerticalOffset="-7">
