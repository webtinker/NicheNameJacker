                                                        ContentTemplate="{StaticResource KeyWordGridButtonMoreContentTemplate}" 
                                                        Command="{Binding DataContext.TogglePopupCommand, ElementName=DomainItemsGrid}"
                                                        CommandParameter="{x:Reference DIGridDeletePopup}"/>

                                                        <Popup x:Name="DIGridDeletePopup"
