                        <WrapPanel Orientation="Horizontal" VerticalAlignment="Center" >
                            <TextBlock Margin="0" Text="Domains " TextWrapping="NoWrap" FontSize="13" Style="{DynamicResource CaptionStyle}" />
                            <TextBlock Margin="0" Text="{Binding TotalDomainsCount}" 
                               TextWrapping="NoWrap" FontSize="13" FontWeight="Bold"
                               Style="{DynamicResource TitleStyle}"/>
                            <TextBlock Margin="12 0 0 0" Text="Available " TextWrapping="NoWrap" FontSize="13" Style="{DynamicResource CaptionStyle}" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=DomainItemsGrid}"/>
                            <TextBlock Margin="0" Text="{Binding AvailableCount}" FontWeight="Bold" TextWrapping="NoWrap" FontSize="13" 
                               Style="{DynamicResource TitleStyle}" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=DomainItemsGrid}"/>
                            <TextBlock Margin="12 0 0 0" Text="Blacklisted " TextWrapping="NoWrap" FontSize="13" Style="{DynamicResource CaptionStyle}" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=BlacklistedItemsGrid}"/>
                            <TextBlock Margin="0" Text="{Binding ItemsSource.Count, ElementName=BlacklistedItemsGrid}" 
                               FontWeight="Bold"
                               TextWrapping="NoWrap" FontSize="13" Style="{DynamicResource TitleStyle}" 
                               Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=BlacklistedItemsGrid}"/>

                            <TextBlock Margin="12 0 0 0" Text="Favorites " TextWrapping="NoWrap" FontSize="13" Style="{DynamicResource CaptionStyle}" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=FavoriteItemsGrid}"/>
