                            ContentTemplate="{StaticResource IconTextPopupButtonContentTemplate}"
                            Command="{Binding TogglePopupCommand}" CommandParameter="{x:Reference InfoPopup}" />
                </StackPanel>


                <StackPanel x:Name="CommandBarDomains" DockPanel.Dock="Right" Orientation="Horizontal" HorizontalAlignment="Right" 
