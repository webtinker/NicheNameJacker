                        <Button Tag="Save available" 
                                Content="&#xE105;" 
                                Style="{StaticResource CommandBarIconTextButtonStyle}"
                                ContentTemplate="{StaticResource IconTextButtonContentTemplate}"
                                Command="{Binding Saver.AvailableCommand}" />
                        <Button Tag="Save selected"
                                Content="&#xE105;" 
                                Style="{StaticResource CommandBarIconTextButtonStyle}"
                                ContentTemplate="{StaticResource IconTextButtonContentTemplate}"
                                Command="{Binding Saver.SelectedCommand}"/>
                    </StackPanel>
