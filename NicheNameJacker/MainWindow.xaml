<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:NicheNameJacker.Controls"
        xmlns:ViewModels="clr-namespace:NicheNameJacker.ViewModels"
        xmlns:usercontrol="clr-namespace:Application.UserControls"
        xmlns:common="clr-namespace:NicheNameJacker.Common"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:ic="clr-namespace:Microsoft.Expression.Interactivity.Core;assembly=Microsoft.Expression.Interactions"
        xmlns:Converters="clr-namespace:NicheNameJacker.Converters" x:Class="Application.MainWindow"
        mc:Ignorable="d"
        Title="{Binding Title}"
        d:DesignWidth="1000"
        MinWidth="800"  Height="1000" Width="1500" MinHeight="600" d:DataContext="{d:DesignInstance {x:Type ViewModels:MainViewModel}, IsDesignTimeCreatable=True}" Icon="Assets/Images/logo-sm.png" WindowState="Maximized" Loaded="Window_Loaded">
    <Window.Resources>


        <!--<Style x:Key="MaterialDesignDataGridCell" TargetType="{x:Type DataGridCell}">
            <Setter Property="Control.Padding" Value="{Binding RelativeSource={RelativeSource Self}, Path=(wpf:DataGridAssist.CellPadding)}" />
            <Setter Property="Control.Background" Value="#00FFFFFF" />
