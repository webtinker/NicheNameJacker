                                                                BorderBrush="White" BorderThickness="1">
                                                                <StackPanel >
                                                                    <Button Style="{StaticResource PopupButtonStyle}"
                                                                        Content="Delete All"
                                                                        Command="{Binding DataContext.ClearCommand, ElementName=BulkCheckerDataGrid, Mode=OneWay}"/>
                                                                    <Button Style="{StaticResource PopupButtonStyle}"
                                                                        Content="Delete Selected"
                                                                        Command="{Binding DataContext.DeleteSelectedDomainsCommand, ElementName=BulkCheckerDataGrid, Mode=OneWay}"/>
