                               TextWrapping="NoWrap" FontSize="13" 
                               Style="{DynamicResource TitleStyle}" 
                               Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=FavoriteItemsGrid}"/>

                            <TextBlock Margin="12 0 0 0" Text="Web 2.0 " TextWrapping="NoWrap" Style="{DynamicResource CaptionStyle}" FontSize="13" />
                            <TextBlock Margin="0" Text="{Binding  Pbn.TotalDomainsCount}" 
                               FontWeight="Bold"
                               TextWrapping="NoWrap" Style="{DynamicResource TitleStyle}" 
                               FontSize="13" />

                            <TextBlock Margin="12 0 0 0" Text="Web 2.0 available " 
                               TextWrapping="NoWrap" Style="{DynamicResource CaptionStyle}" FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnDataGrid}"/>
                            <TextBlock Margin="0" Text="{Binding Pbn.AvailableCount}" 
                               FontWeight="Bold"
                               TextWrapping="NoWrap" Style="{DynamicResource TitleStyle}" 
                               FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnDataGrid}"/>

                            <TextBlock Margin="12 0 0 0" Text="Web 2.0 blacklisted " TextWrapping="NoWrap" Style="{DynamicResource CaptionStyle}" FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnBlacklistedDataGrid}"/>
                            <TextBlock Margin="0" Text="{Binding ItemsSource.Count, ElementName=PbnBlacklistedDataGrid}" 
                               FontWeight="Bold"
                               TextWrapping="NoWrap" Style="{DynamicResource TitleStyle}" 
                               FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnBlacklistedDataGrid}"/>

                            <TextBlock Margin="12 0 0 0" Text="Web 2.0 favorites " TextWrapping="NoWrap" Style="{DynamicResource CaptionStyle}" FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnFavoriteGridView}"/>
                            <TextBlock Margin="0" Text="{Binding ItemsSource.Count, ElementName=PbnFavoriteGridView}" 
                               FontWeight="Bold"
                               TextWrapping="NoWrap" Style="{DynamicResource TitleStyle}" 
                               FontSize="13" Visibility="{Binding Items.IsEmpty, ConverterParameter=true, Converter={StaticResource BoolToVisibilityConverter}, ElementName=PbnFavoriteGridView}"/>
                        </WrapPanel>
                    </Border>

                </Popup>

                <Popup x:Name="SaveActionPopup"
					PopupAnimation="Slide" Placement="Top"
					PlacementTarget="{Binding ElementName=SaveButton}" 
					StaysOpen="False" AllowsTransparency="True" VerticalOffset="-1">
                    <StackPanel Background="{DynamicResource AppBarBackground}" Orientation="Horizontal"
						HorizontalAlignment="Center" VerticalAlignment="Center"
						Width="Auto">
                        <Button x:Name="SaveAllButton" 
							Content="&#xE105;" 
                            Style="{StaticResource CommandBarIconTextButtonStyle}"
                            ContentTemplate="{StaticResource IconTextButtonContentTemplate}"
							FontFamily="{StaticResource SegoeUISymbol}"
							ToolTip="Save available domains to file"
							Command="{Binding SaveAllDomainsCommand, Mode=OneWay}"
							Tag="Save available"/>
                        <Button x:Name="SaveSelectedButton" 
							Content="&#xE105;" 
                            Style="{StaticResource CommandBarIconTextButtonStyle}"
                            ContentTemplate="{StaticResource IconTextButtonContentTemplate}"
							ToolTip="Save selected domains to file"
							Command="{Binding SaveSelectedDomainsCommand, Mode=OneWay}"
							Tag="Save selected"/>
                    </StackPanel>
                </Popup>
