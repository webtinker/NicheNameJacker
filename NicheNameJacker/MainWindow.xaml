        <Converters:MathConverter x:Key="MathConverter"/>
        <Converters:SpecifiedIntValueToVisibilityConverter x:Key="SpecifiedIntValueToVisibilityConverter"/>
        <Converters:MultiBooleanToVisibilityConverter x:Key="MultiBooleanToVisibilityConverter"/>


        <DataTemplate x:Key="GenericButtonContentTemplate">
            <StackPanel Orientation="Vertical">
                <TextBlock Text="{Binding Mode=OneWay}" FontSize="15" FontFamily="{StaticResource SegoeUISymbol}" HorizontalAlignment="Center"/>
                <TextBlock TextWrapping="NoWrap" Text="{Binding Tag, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}" FontSize="9" FontFamily="Segoe UI" TextTrimming="CharacterEllipsis" HorizontalAlignment="Center" Margin="0,4,0,0"/>
            </StackPanel>
        </DataTemplate>

        <DataTemplate x:Key="PopupButtonContentTemplate">
            <Grid Background="Transparent">
                <TextBlock  Text="{Binding}" TextWrapping="NoWrap"  FontSize="9" TextTrimming="CharacterEllipsis" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0" Padding="4,2"/>
            </Grid>
        </DataTemplate>

        <Style x:Key="PopupButtonStyle" TargetType="{x:Type Button}" BasedOn="{StaticResource ModernButton}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="ContentTemplate" Value="{StaticResource PopupButtonContentTemplate}"/>
        </Style>

        <Style x:Key="CommandBarButton" TargetType="{x:Type Button}" BasedOn="{StaticResource ModernButton}">
            <Setter Property="FontFamily" Value="{StaticResource SegoeUISymbol}"/>
            <Setter Property="FontSize" Value="15"/>
            <Setter Property="Height" Value="48"/>
            <Setter Property="MinWidth" Value="48"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="ContentTemplate" Value="{DynamicResource GenericButtonContentTemplate}"/>
            <Setter Property="BorderThickness" Value="0"/>

        </Style>
        <Style x:Key="GeneralListViewStyle" TargetType="{x:Type ListView}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="ItemContainerStyle" Value="{DynamicResource ModernNonFocusableListViewItem}"/>
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Hidden"/>
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Hidden"/>
            <Setter Property="VirtualizingPanel.ScrollUnit" Value="Pixel"/>
        </Style>
        <Style x:Key="LinkButton" TargetType="{x:Type Button}" BasedOn="{StaticResource ModernButton}">
            <Setter Property="Margin" Value="0 0 0 3"/>
            <Setter Property="BorderBrush" Value="#FF858585"/>
            <Setter Property="Padding" Value="10 8 0 10"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
        </Style>
        <Style x:Key="AvailableTextBlock" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource CaptionStyle}">
            <Style.Triggers>
                <DataTrigger Binding="{Binding DataContext.IsAvailable, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Value="true">
                    <Setter Property="Foreground" Value="White" />
                </DataTrigger>
                <DataTrigger Binding="{Binding DataContext.IsAvailable, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Value="{x:Null}">
                    <Setter Property="Foreground" Value="{StaticResource CaptionForeground}" />
                </DataTrigger>
                <DataTrigger Binding="{Binding DataContext.IsAvailable, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Value="false">
                    <Setter Property="Foreground" Value="{StaticResource CaptionForeground}" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="AvailableCommandBarButton" TargetType="{x:Type Button}" BasedOn="{StaticResource CommandBarButton}">
            <Style.Triggers>
                <DataTrigger Binding="{Binding IsAvailable}" Value="true">
                    <Setter Property="Foreground" Value="White" />
                </DataTrigger>
                <DataTrigger Binding="{Binding IsAvailable}" Value="{x:Null}">
                    <Setter Property="Foreground" Value="#FF4C4C4C" />
                </DataTrigger>
                <DataTrigger Binding="{Binding IsAvailable}" Value="false">
                    <Setter Property="Foreground" Value="#FF4C4C4C" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <x:Array x:Key="items" Type="{x:Type ViewModels:SingleDomain}">
            <ViewModels:SingleDomain IsAvailable="True" Address="Address" DomainSource="source" Stats="stats, stats, stats"/>
        </x:Array>


        <Visibility x:Key="Visibility.Collapsed">Collapsed</Visibility>
        <Style x:Key="DataGridDomainItemsRowCustomStyle" TargetType="{x:Type DataGridRow}">
            <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
            <Setter Property="SnapsToDevicePixels" Value="true"/>
            <Setter Property="Validation.ErrorTemplate" Value="{x:Null}"/>
            <Setter Property="ValidationErrorTemplate">
                <Setter.Value>
                    <ControlTemplate>
                        <TextBlock Foreground="Red" Margin="2,0,0,0" Text="!" VerticalAlignment="Center"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type DataGridRow}">
                        <Border x:Name="DGR_Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                            <SelectiveScrollingGrid>
                                <SelectiveScrollingGrid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </SelectiveScrollingGrid.ColumnDefinitions>
                                <SelectiveScrollingGrid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </SelectiveScrollingGrid.RowDefinitions>
                                <DataGridCellsPresenter Grid.Column="1" ItemsPanel="{TemplateBinding ItemsPanel}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                                <DataGridDetailsPresenter Grid.Column="1" Grid.Row="1" SelectiveScrollingGrid.SelectiveScrollingOrientation="{Binding AreRowDetailsFrozen, ConverterParameter={x:Static SelectiveScrollingOrientation.Vertical}, Converter={x:Static DataGrid.RowDetailsScrollingConverter}, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}" Visibility="{TemplateBinding DetailsVisibility}"/>
                                <DataGridRowHeader Grid.RowSpan="2" SelectiveScrollingGrid.SelectiveScrollingOrientation="Vertical" Visibility="{Binding HeadersVisibility, ConverterParameter={x:Static DataGridHeadersVisibility.Row}, Converter={x:Static DataGrid.HeadersVisibilityConverter}, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}"/>
                                <ProgressBar Grid.Row="2" Grid.ColumnSpan="2" Style="{DynamicResource ModernProgressBar}" Foreground="DeepSkyBlue"  VerticalAlignment="Top" IsIndeterminate="True" Margin="0,-2,0,0" 
                                             >
                                    <ProgressBar.Visibility>
                                        <PriorityBinding>
                                            <Binding Path="IsChecking" Converter="{StaticResource BoolToVisibilityConverter}"/>
                                            <Binding Source="{StaticResource Visibility.Collapsed}"/>
                                        </PriorityBinding>

                                    </ProgressBar.Visibility>

                                </ProgressBar>
                            </SelectiveScrollingGrid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsNewItem" Value="True">
                    <Setter Property="Margin" Value="{Binding NewItemMargin, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ThumbStyle1" TargetType="{x:Type Thumb}">
            <Setter Property="Width" Value="8"/>
            <Setter Property="Cursor" Value="SizeWE"/>
            <Style.BasedOn>
                <Style TargetType="{x:Type Thumb}">
                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                    <Setter Property="Padding" Value="8"/>
                    <Setter Property="Background" Value="Transparent"/>
                    <Setter Property="OverridesDefaultStyle" Value="True"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Thumb}">
                                <Border x:Name="PART_Border" Padding="{TemplateBinding Padding}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Background="{TemplateBinding Background}"/>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Style.BasedOn>
        </Style>
        <Style x:Key="DataGridColumnHeaderStyle1" TargetType="{x:Type DataGridColumnHeader}">
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
            <Setter Property="TextBlock.FontSize" Value="12" />
            <Setter Property="TextBlock.FontWeight" Value="Medium" />
            <Setter Property="Padding" Value="{Binding (materialDesign:DataGridAssist.ColumnHeaderPadding), RelativeSource={RelativeSource Self}}" />
            <Setter Property="Foreground" Value="{DynamicResource MaterialDesignColumnHeader}" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type DataGridColumnHeader}">
                        <ControlTemplate.Resources>
                            <Storyboard x:Key="GoToVisible">
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SortDirectionIndicator" Storyboard.TargetProperty="Width">
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="0" />
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="12" />
                                </DoubleAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="GoToHidden">
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SortDirectionIndicator" Storyboard.TargetProperty="Width">
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="12" />
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0" />
                                </DoubleAnimationUsingKeyFrames>
                            </Storyboard>
                        </ControlTemplate.Resources>
                        <Grid>
                            <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}">
                                <Grid Background="#00FFFFFF">
                                    <!--<Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>-->
                                    <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" 
                                                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" 
                                                      Margin="{TemplateBinding Padding}" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <materialDesign:ListSortDirectionIndicator x:Name="SortDirectionIndicator" Width="0" Height="12" ListSortDirection="{TemplateBinding SortDirection}" />
                                        <ContentPresenter Grid.Column="1"
                                                      RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                    </Grid>
                                </Grid>
                            </Border>
                            <Thumb x:Name="PART_LeftHeaderGripper" HorizontalAlignment="Left" Style="{DynamicResource ThumbStyle1}" />
                            <Thumb x:Name="PART_RightHeaderGripper" HorizontalAlignment="Right" Style="{StaticResource ThumbStyle1}" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Value="{DynamicResource MaterialDesignBody}" Property="Foreground" />
                            </Trigger>
                            <Trigger Property="SortDirection" Value="Ascending">
                                <Setter Value="{DynamicResource MaterialDesignBody}" Property="Foreground" />
                            </Trigger>
                            <Trigger Property="SortDirection" Value="Descending">
                                <Setter Value="{DynamicResource MaterialDesignBody}" Property="Foreground" />
                            </Trigger>
                            <Trigger SourceName="SortDirectionIndicator" Property="IsNeutral" Value="False">
                                <Trigger.EnterActions>
                                    <BeginStoryboard x:Name="GoToVisible" Storyboard="{StaticResource GoToVisible}" />
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <RemoveStoryboard BeginStoryboardName="GoToVisible" />
                                    <BeginStoryboard Storyboard="{StaticResource GoToHidden}" />
                                </Trigger.ExitActions>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="ColumnHeaderGripperStyle" TargetType="{x:Type Thumb}">
            <Setter Property="Width" Value="8"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Cursor" Value="SizeWE"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                        <Border Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="DataGridColumnHeaderRightAlignStyle" TargetType="{x:Type DataGridColumnHeader}" BasedOn="{StaticResource DataGridColumnHeaderStyle1}">
            <Setter Property="HorizontalContentAlignment" Value="Right" />
        </Style>

        <Style x:Key="SearchOptionsCheckboxStyle" TargetType="{x:Type CheckBox}" BasedOn="{StaticResource MaterialDesignCheckBox}">
            <Setter Property="Foreground" Value="Orange"/>
            <Setter Property="Background" Value="Orange"/>
            <Style.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Property="Foreground" Value="Orange"/>
                </Trigger>
                <Trigger Property="IsChecked" Value="False">
                    <Setter Property="Foreground" Value="White"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="SidePanelHyperlinkTextBlockStyle" TargetType="{x:Type TextBlock}">
            <Setter Property="Margin" Value="4"/>
            <Setter Property="FontSize" Value="12" />
            <Setter Property="HorizontalAlignment" Value="Left"/>
        </Style>

        <Style x:Key="SidePanelHyperLinkStyle" TargetType="{x:Type Hyperlink}">
            <Setter Property="Foreground" Value="White" />
            <Setter Property="TextDecorations" Value=""/>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <!--<Setter Property="Foreground" Value="#FF010115"/>-->
                    <Setter Property="TextDecorations" Value="Underline"/>
                </Trigger>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                </Trigger>
                <Trigger Property="IsEnabled" Value="True">
                    <Setter Property="Cursor" Value="Hand"/>
                </Trigger>
            </Style.Triggers>
        </Style>


        <Style x:Key="AppGenericTabItemStyle" TargetType="{x:Type TabItem}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Grid x:Name="Panel" Width="140">
                            <Border x:Name="Border" BorderThickness="2,3,3,0" BorderBrush="#FF535353" CornerRadius="0,4,0,0" Margin="2,0" Background="#FF535353">
                                <ContentPresenter x:Name="ContentSite"  VerticalAlignment="Center"  HorizontalAlignment="Center" ContentSource="Header" Margin="10,2"/>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="Border" Property="Background" Value="#FF535353"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="Border" Property="Background" Value="#FF303030"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="HeaderTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Grid>
                            <TextBlock Text="{Binding}" FontSize="12" Foreground="Orange"/>
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="SimpleProgressStyle" TargetType="{x:Type usercontrol:SimpleProgress}">
            <Setter Property="ProgressBackground" Value="Gray"/>
            <Setter Property="ProgressForeground" Value="Orange"/>
            <Setter Property="HorizontalAlignment" Value="Right"/>
        </Style>


        <Style x:Key="CommandBarIconTextButtonStyle" TargetType="{x:Type Button}" BasedOn="{StaticResource ModernButton}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="10"/>
            <Setter Property="VerticalAlignment" Value="Top"/>
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="Padding" Value="0,4"/>
            <Setter Property="Width" Value="48"/>
        </Style>

        <DataTemplate x:Key="IconTextButtonContentTemplate">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="4"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <TextBlock Text="{Binding}" FontSize="18" FontFamily="Segoe MDL2 Assets" HorizontalAlignment="Center"/>
                <TextBlock Grid.Row="2" TextWrapping="Wrap" TextAlignment="Center"
                           Text="{Binding Tag, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}" 
                           FontSize="{Binding FontSize, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}"
                           HorizontalAlignment="Center"/>
            </Grid>
        </DataTemplate>

        <DataTemplate x:Key="IconTextPopupButtonContentTemplate">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="4"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="&#xE712;" FontFamily="Segoe MDL2 Assets" FontSize="14"
                                   VerticalAlignment="Center" 
                                   LineHeight="8" LineStackingStrategy="BlockLineHeight"
                                   TextAlignment="Center" HorizontalAlignment="Left" Margin="0,0,-4,0" >
                        <TextBlock.LayoutTransform>
                            <RotateTransform Angle="-90"/>
                        </TextBlock.LayoutTransform>
                    </TextBlock>
                    <TextBlock Text="{Binding}" Grid.Column="1" FontSize="18" FontFamily="Segoe MDL2 Assets" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Grid>

                <TextBlock Grid.Row="2" TextWrapping="Wrap" TextAlignment="Center"
                           Text="{Binding Tag, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}"
                           FontSize="{Binding FontSize, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}"
                           HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Grid>
        </DataTemplate>

    </Window.Resources>
    <Grid Background="{DynamicResource AppBackground}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="1"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <controls:AppUpdaterPanel Grid.Row="0" Grid.ColumnSpan="3" DataContext="{Binding AppUpdater}"/>

        <Grid Grid.Row="1" Width="330" Background="{DynamicResource AppBarBackground}" Margin="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="48"/>
            </Grid.RowDefinitions>
            <Grid x:Name="FindDomainControls" Visibility="{Binding SelectedIndex, ConverterParameter=0;1;2, Converter={StaticResource SpecifiedIntValueToVisibilityConverter}, ElementName=TabControl}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Vertical" Margin="12" d:LayoutOverrides="Height">
                    <Label Content="Search for domains" Style="{DynamicResource ModernLabel}"/>
                    <StackPanel Margin="0,4,0,0">
                        <StackPanel Orientation="Horizontal" d:LayoutOverrides="Width" >
                            <TextBox x:Name="SearchTextBox" 
						Style="{DynamicResource ModernTextBox}" Foreground="White" Width="194"
                        Text="{Binding SearchQuery, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}" Background="#FF020E00" >
                                <TextBox.InputBindings>
                                    <KeyBinding Command="{Binding SearchCommand}" Key="Return"/>
                                </TextBox.InputBindings>
                            </TextBox>
                            <!--  -->
                            <controls:NumericTextBox x:Name="MaxResultsField" 
						Style="{DynamicResource ModernTextBox}" Text="{Binding MaxResults}" 
						ToolTip="Maximum Results"
						Width="32" MaxLength="3" HorizontalContentAlignment="Center" Margin="2,0,0,0" />
                            <StackPanel Orientation="Horizontal" x:Name="ButtonsPanelFindDomains" IsEnabled="{Binding CanSearchNow}" >

                                <Button x:Name="SearchButton" 
						Content="&#xE11A;" Style="{DynamicResource ModernButton}"
						FontFamily="{StaticResource SegoeUISymbol}" Foreground="Orange" 
                            Command="{Binding SearchCommand, Mode=OneWay}" 
                            IsEnabled="{Binding Text, Converter={StaticResource TextToBoolConverter}, ElementName=SearchTextBox}" 
                            BorderThickness="0" Width="32" Height="32" Margin="2,0,0,0" Click="SearchButton_Click" />
                                <Button x:Name="BulkUploadButon" 
						Content="&#xE17C;" 
                                ToolTipService.ShowOnDisabled="{Binding CanSearchNow}"
                                IsEnabled="{Binding Permissions.Nnj.CanBulkSearch}"
						FontFamily="{StaticResource SegoeUISymbol}" Foreground="White" 
                            Command="{Binding SearchBulkCommand, Mode=OneWay}" 
                            BorderThickness="0" Width="32" Height="32" Margin="2,0,0,0">
                                    <Button.Style>
                                        <Style TargetType="{x:Type Button}" BasedOn="{StaticResource ModernButton}">
                                            <Setter Property="Foreground" Value="#FFF1F2F7"/>
                                            <Style.Triggers>
                                                <Trigger Property="IsEnabled" Value="False">
                                                    <Setter Property="Foreground" Value="#FF898989"/>
                                                    <Setter Property="ToolTip">
                                                        <Setter.Value>
                                                            <TextBlock Text="{Binding Permissions.MembershipPlanName, StringFormat=This option is not available in the \{0\} version}"/>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>
                                                <Trigger Property="IsEnabled" Value="True">
                                                    <Setter Property="ToolTip">
                                                        <Setter.Value>
                                                            <TextBlock Text="Bulk keyword upload"/>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Button.Style>
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        <TextBlock Text="{Binding BulkInfo}" Style="{DynamicResource CaptionStyle}" 
                               Visibility="{Binding SearchBulk, Converter={StaticResource BoolToVisibilityConverter}}" />
                    </StackPanel>

                    <Label Content="Search from:" Style="{DynamicResource ModernLabel}" Margin="0,8,0,0"/>
                    <UniformGrid Margin="0,2,0,0">
                        <CheckBox Content="YouTube" IsChecked="{Binding SearchInYouTube}" 
                            IsEnabled="{Binding CanScrapeYouTube}"
                            ToolTipService.ShowOnDisabled="True" Margin="0,0,27,0">
                            <CheckBox.Style>
                                <Style TargetType="{x:Type CheckBox}" BasedOn="{StaticResource SearchOptionsCheckboxStyle}">
                                    <Style.Triggers>
                                        <Trigger Property="IsEnabled" Value="False">
                                            <Setter Property="ToolTip">
                                                <Setter.Value>
                                                    <TextBlock Text="{Binding YouTubeToolTipText}"/>
                                                </Setter.Value>
                                            </Setter>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </CheckBox.Style>
                        </CheckBox>
                        <CheckBox Content="Reddit" IsChecked="{Binding SearchInReddit}" Style="{StaticResource SearchOptionsCheckboxStyle}"/>
                        <CheckBox Content="Tumblr" IsChecked="{Binding SearchInTumblr}"  Style="{StaticResource SearchOptionsCheckboxStyle}"/>
                        <CheckBox Content="Wikipedia" 
                            IsChecked="{Binding SearchWiki}"
                            IsEnabled="{Binding Permissions.Nnj.CanScrapeWiki}"
                            ToolTipService.ShowOnDisabled="True">
                            <CheckBox.Style>
                                <Style TargetType="{x:Type CheckBox}" BasedOn="{StaticResource SearchOptionsCheckboxStyle}">
                                    <Style.Triggers>
                                        <Trigger Property="IsEnabled" Value="False">
