                                                                <Style>
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding (IsMouseOver), RelativeSource={RelativeSource AncestorType={x:Type DataGridRow}}}" Value="True">
                                                                            <Setter Property="UIElement.Visibility" Value="Visible" />
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding (IsMouseOver), RelativeSource={RelativeSource AncestorType={x:Type DataGridRow}}}" Value="False">
                                                                            <Setter Property="UIElement.Visibility" Value="Hidden" />
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </Grid.Style>

